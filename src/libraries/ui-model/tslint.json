{
  "rules": {
    "no-reference": true,
    "no-var-requires": true,
    "only-arrow-functions": false,
    "prefer-for-of": true,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "property-declaration",
      "member-variable-declaration"
    ],
    "unified-signatures": true,
    "curly": true,
    "forin": true,
    "label-position": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-console": [
      true,
      "log",
      "info",
      "error"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-eval": true,
    "no-invalid-this": true,
    "no-null-keyword": true,
    "no-string-throw": true,
    "no-unused-expression": true,
    "no-unused-new": true,
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "radix": true,
    "triple-equals": true,
    "use-isnan": true,
    "cyclomatic-complexity": true,
    "indent": true,
    "linebreak-style": [
      false,
      "LF"
    ],
    "max-classes-per-file": [
      true,
      1
    ],
    "no-require-imports": true,
    "prefer-const": true,
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "arrow-parens": true,
    "class-name": true,
    "new-parens": true,
    "no-angle-bracket-type-assertion": true,
    "no-consecutive-blank-lines": true,
    "one-line": [
      true,
      "check-catch",
      "check-open-brace",
      "check-else",
      "check-whitespace"
    ],
    "one-variable-per-declaration": true,
    "semicolon": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
